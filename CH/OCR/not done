pin = "4214"
dan = input("Input the four digits in question: ")
numbers = [dan[0], dan[1], dan[2], dan[3]]
saved = []
print(numbers)

for i in range(4):
    for j in range(4):
        for k in range(4):
            for l in range(4):
                attempt = dan[i] + dan[j] + dan[k] + dan[l]
                if attempt in saved:
                    continue
                elif attempt.count(dan[0]) > 1 or attempt.count(dan[1]) > 1 or attempt.count(dan[2]) > 1 or attempt.count(dan[3]) > 1:
                    continue
                else:
                    if attempt == pin:
                        print("PIN HAS BEEN CRACKED: ", attempt)
                        break
                    else:
                        print(attempt)
                        saved.append(attempt)
            if attempt == pin:
                break

        if attempt == pin:
            break
        
    if attempt == pin:
        break
